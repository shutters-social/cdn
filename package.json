{
  "name": "@shutters/cdn",
  "private": true,
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "clean": "rimraf --no-interactive dist",
    "build": "bun build . --compile --outfile=dist/cdn",
    "build:clean": "bun run clean && bun run build",
    "typecheck": "tsc",
    "test": "bun test",
    "ci:codequality": "biome ci .",
    "format": "biome format",
    "format:fix": "biome format --write",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "check": "biome check",
    "check:fix": "biome check --write",
    "deploy": "fly deploy --build-arg \"vcs_ref=$(git rev-parse --short HEAD)\""
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@atcute/cid": "^2.1.0",
    "@aws-sdk/client-dynamodb": "^3.744.0",
    "@aws-sdk/client-s3": "^3.744.0",
    "@aws-sdk/lib-dynamodb": "^3.744.0",
    "@hono/sentry": "^1.2.0",
    "@sentry/bun": "^8.54.0",
    "@shutters/shutterkit": "0.2.2"
  }
}
